{
  "name": "docs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "apidoc": "^0.27.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  },
  "watch": {
    "test": {
      "extensions": "js",
      "patterns": [
        "docs",
        "docs/*",
        "docs/*/*"
      ]
    }
  },
  "scripts": {
    "doctest": "npx apidoc -e node_modules/ -o ~/Desktop/createdDocs",
    "nodemon:docs": "npx nodemon --exec \"npm run doctest\" ./docs/*.js"
  },
